{
	"name": "@dwimm/server",
	"description": "",
	"bin": {
		"dwimm-db": "./bin/database",
		"dwimm-plugin": "./bin/plugin",
		"dwimm-server": "./server.js"
	},
	"scripts": {
		"check": "npm run check:eslint && npm run check:npm && npm run check:nsp",
		"check:quick": "npm run check:eslint",
		"check:eslint": "eslint ./",
		"check:nsp": "nsp check || true",
		"check:npm": "npm-check -s",
		"visualize": "plato -rt dwimm-server -d ./code-visualization -e \".eslintrc.json\" -x 'code-visualization|node_modules' ./**/*.js",
		"bump:reset": "export MODULEVERSION=$(npm view @dwimm/server version) && npm run bump:reset:save",
		"bump:reset:next": "export MODULEVERSION=$(npm view @dwimm/server@next version) && npm run bump:reset:save",
		"bump:reset:save": "node ./.ci-update-version",
		"bump:develop": "npm run bump:reset:next && npm --no-git-tag-version version prerelease",
		"bump:production": "npm run bump:reset && npm --no-git-tag-version version from-git"
	},
	"author": "Sebastian Pekarek <mail@sebbo.net>",
	"dependencies": {
		"basic-auth": "^2.0.0",
		"bcrypt": "^1.0.3",
		"body-parser": "^1.18.2",
		"bunyan": "^1.8.12",
		"cli-table": "^0.3.1",
		"commander": "^2.15.0",
		"email-validator": "^1.1.1",
		"express": "^4.16.3",
		"moment": "^2.21.0",
		"mysql2": "^1.5.2",
		"promised-exec": "^1.0.1",
		"raven": "^2.4.2",
		"request-promise-native": "^1.0.5",
		"sequelize": "^4.37.0",
		"shell-escape": "^0.2.0",
		"socket.io": "^2.0.4",
		"umzug": "^2.1.0",
		"underscore": "^1.8.3",
		"uuid": "^3.2.1"
	},
	"devDependencies": {
		"eslint": "^4.18.2",
		"eslint-plugin-security": "^1.4.0",
		"npm-check": "^5.5.2",
		"nsp": "^3.2.1",
		"plato": "^1.7.0"
	}
}
